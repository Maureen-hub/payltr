{
  "ConnectionStrings": {
    "DefaultConnection": "server={HostName};port={Port};database={DatabaseName};user={User};password={Password}"
  },
  "Logging": {
    "LogLevel": { "Default": "Information", "Microsoft.AspNetCore": "Warning" }
  },
  "JwtSettings": {
    "Secret": "YOUR_VERY_SECURE_SECRET_KEY_HERE",
    "Issuer": "PayltrApi",
    "Audience": "PayltrClient",
    "ExpiryMinutes": 60
  },
  "Cors": {
    "AllowedOrigins": ["http://localhost:5100", "http://192.168.2.41:5100"]
  },
  "AllowedHosts": "*"
}
